#include <iostream>

void main()
{
	int b[5] = { 10,20,30,40,50 };
	
	std::cout << b[0] << '\n' << b[1] << '\n' << b[2] << '\n' << b[3] << '\n' << b[4];
}

//IPSISUJE svakog clana

#include <iostream>

void main()
{
 int b[5] = { 10,20,30,40,50 };

 for (int pozicijauarrayu = 0; pozicijauarrayu < 5; pozicijauarrayu += 2)

	std::cout << b[pozicijauarrayu] << '\n';

}

//ISPISUJE svakog drugog clana


void main()
{
	float x[] = {10.5, 11.5, 12.5, 13.5, 14.5};
	float a = x[3];
	std::cout << a;

}


ISPISUJE vrijednost x na trecoj poziciji, ispisuje 13.


void main()
{
	float x[] = { 13.5, 4.5, 3.5, 6.5, 77.5 };

	x[2] = x[0] + x[1];
	std::cout << x[2];
}


ISPISUJE 18

void main()
{
	float x[] = { 13.5, 4.5, 3.5, 6.5, 77.5 };
	x[3]++;
	std::cout << x[3];
}

ISPISUJE 7.5
