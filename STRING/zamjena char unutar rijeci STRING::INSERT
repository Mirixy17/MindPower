int main()
{
	std::string str = "MeinLiebe";
	std::string str2 = "e ";
	str.insert(4, str2);
	std::cout << str;
}


#include <iostream>
#include <string>

int main()
{
	std::string MyLukic = "MeinLiebe";
	std::string MyLukic4 = "e";
	MyLukic.insert(4, MyLukic4);
	std::string MyLukic5 = " ";
	MyLukic.insert(5, MyLukic5);
	std::cout << MyLukic;
}

